{
  "name": "tomahawk",
  "productName": "tomahawk",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Dima",
    "email": "zgltyq@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/icons/tomahawk.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tomahawk"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "detect-port": "^1.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "random-useragent": "^0.5.0",
    "request": "^2.88.2",
    "tor-request": "^3.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "babel-eslint": "^10.1.0",
    "electron": "17.1.2",
    "eslint": "^7.32.0",
    "eslint-config-webbylab": "^5.3.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-more": "0.3.2",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-security": "^1.4.0"
  }
}
